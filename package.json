{
  "name": "confc",
  "version": "3.2.0",
  "description": "Start new project with your default configs.",
  "files": [
    "dist",
    "files.yaml"
  ],
  "main": "./dist/confc.cjs.js",
  "module": "./dist/confc.js",
  "typings": "./dist/confc.d.ts",
  "engines": {
    "node": ">=8"
  },
  "preferGlobal": true,
  "bin": {
    "confc": "./dist/cli.js"
  },
  "scripts": {
    "prebuild": "rimraf dist/*",
    "build": "tsc",
    "postbuild": "ts-node ./scripts/chmodCLI.ts",
    "format": "prettier --write \"{src,test}/**/*.ts\"",
    "lint": "tslint -p .",
    "clean-test": "ts-node ./scripts/cleanTests.ts",
    "pretest": "run-s build clean-test",
    "test": "npm run lint && jest --verbose",
    "posttest": "run-s clean-test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gluons/ConfC.git"
  },
  "keywords": [
    "conf",
    "config",
    "configuration"
  ],
  "author": "Saran Tanpituckpong <sarunta@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gluons/ConfC/issues"
  },
  "homepage": "https://github.com/gluons/ConfC",
  "dependencies": {
    "chalk": "^2.4.2",
    "fs-extra": "^7.0.1",
    "inquirer": "^6.3.1",
    "js-yaml": "^3.13.1",
    "nvl": "^1.2.2",
    "p-series": "^2.1.0",
    "p-waterfall": "^2.1.0",
    "rc": "^1.2.2",
    "update-notifier": "^2.3.0",
    "yargs": "^13.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-typescript": "^7.3.3",
    "@gluons/prettier-config": "^2.0.0",
    "@types/cross-spawn": "^6.0.0",
    "@types/fs-extra": "^5.0.5",
    "@types/glob": "^7.1.1",
    "@types/jest": "^24.0.11",
    "@types/js-yaml": "^3.12.1",
    "@types/node": "6",
    "@types/yargs": "^13.0.0",
    "cross-spawn": "^6.0.0",
    "del": "^4.1.0",
    "jest": "^24.7.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.17.0",
    "rimraf": "^2.6.3",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "tslint-config-gluons": "^2.1.0",
    "typescript": "^3.4.3"
  }
}
