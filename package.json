{
  "name": "confc",
  "version": "3.2.0",
  "description": "Start new project with your default configs.",
  "files": [
    "dist",
    "files.yaml"
  ],
  "main": "./dist/confc.cjs.js",
  "module": "./dist/confc.js",
  "typings": "./dist/confc.d.ts",
  "engines": {
    "node": ">=8"
  },
  "preferGlobal": true,
  "bin": {
    "confc": "./dist/cli.js"
  },
  "scripts": {
    "prebuild": "rimraf dist/*",
    "build": "tsc",
    "postbuild": "ts-node ./scripts/chmodCLI.ts",
    "format": "prettier --write \"{src,test}/**/*.ts\"",
    "lint": "tslint -p .",
    "clean-test": "ts-node ./scripts/cleanTests.ts",
    "pretest": "run-s build clean-test",
    "test": "npm run lint && jest --verbose",
    "posttest": "run-s clean-test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gluons/ConfC.git"
  },
  "keywords": [
    "conf",
    "config",
    "configuration"
  ],
  "author": "Saran Tanpituckpong <sarunta@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gluons/ConfC/issues"
  },
  "homepage": "https://github.com/gluons/ConfC",
  "dependencies": {
    "chalk": "^4.1.0",
    "fs-extra": "^9.0.1",
    "inquirer": "^7.3.2",
    "js-yaml": "^3.14.0",
    "nvl": "^1.2.2",
    "p-series": "^2.1.0",
    "p-waterfall": "^2.1.0",
    "rc": "^1.2.2",
    "update-notifier": "^5.0.0",
    "yargs": "^17.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@gluons/prettier-config": "^2.0.3",
    "@types/cross-spawn": "^6.0.2",
    "@types/glob": "^7.1.3",
    "@types/jest": "^26.0.4",
    "@types/js-yaml": "^3.12.5",
    "@types/node": "10",
    "@types/yargs": "^17.0.0",
    "cross-spawn": "^7.0.3",
    "del": "^6.0.0",
    "jest": "^26.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.2",
    "tslint-config-gluons": "^2.2.0",
    "typescript": "^4.0.0"
  }
}
