{
  "name": "confc",
  "version": "3.2.0",
  "description": "Start new project with your default configs.",
  "files": [
    "dist",
    "files.yaml"
  ],
  "main": "./dist/confc.cjs.js",
  "module": "./dist/confc.js",
  "typings": "./dist/confc.d.ts",
  "engines": {
    "node": ">=6"
  },
  "preferGlobal": true,
  "bin": {
    "confc": "./dist/cli.js"
  },
  "scripts": {
    "prebuild": "rimraf dist/*",
    "build": "tsc",
    "postbuild": "ts-node ./scripts/chmodCLI.ts",
    "format": "prettier --write \"{src,test}/**/*.ts\"",
    "lint": "tslint -p .",
    "clean-test": "ts-node ./scripts/cleanTests.ts",
    "pretest": "run-s build clean-test",
    "test": "npm run lint && mocha --require ts-node/register \"test/*.test.ts\"",
    "posttest": "run-s clean-test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gluons/ConfC.git"
  },
  "keywords": [
    "conf",
    "config",
    "configuration"
  ],
  "author": "Saran Tanpituckpong <sarunta@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gluons/ConfC/issues"
  },
  "homepage": "https://github.com/gluons/ConfC",
  "dependencies": {
    "chalk": "^2.3.0",
    "fs-extra": "^7.0.0",
    "inquirer": "^6.2.0",
    "js-yaml": "^3.6.1",
    "nvl": "^1.1.0",
    "p-series": "^1.0.0",
    "p-waterfall": "^1.0.0",
    "rc": "^1.2.2",
    "update-notifier": "^2.3.0",
    "yargs": "^13.0.0"
  },
  "devDependencies": {
    "@gluons/prettier-config": "^2.0.0",
    "@types/chai": "^4.1.6",
    "@types/fs-extra": "^5.0.4",
    "@types/js-yaml": "^3.11.2",
    "@types/mocha": "^5.2.5",
    "@types/node": "6",
    "@types/yargs": "^12.0.1",
    "chai": "^4.2.0",
    "chai-files": "^1.4.0",
    "cross-spawn": "^6.0.0",
    "del": "^4.0.0",
    "mocha": "*",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.14.3",
    "rimraf": "^2.6.2",
    "ts-node": "^8.0.0",
    "tslint": "^5.11.0",
    "tslint-config-gluons": "^2.0.3",
    "typescript": "^3.1.3"
  }
}
