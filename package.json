{
  "name": "confc",
  "version": "3.0.0",
  "description": "Start new project with your default configs.",
  "files": [
    "dist",
    "types"
  ],
  "main": "./dist/confc.js",
  "engines": {
    "node": ">=6"
  },
  "preferGlobal": true,
  "bin": {
    "confc": "./bin/confc.js"
  },
  "scripts": {
    "prebuild": "rimraf dist/*",
    "build": "tsc -p .",
    "lint": "tslint -p .",
    "clean-test": "node ./scripts/clean-test.js",
    "pretest": "npm run clean-test",
    "test": "mocha",
    "posttest": "npm run clean-test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gluons/ConfC.git"
  },
  "keywords": [
    "conf",
    "config",
    "configuration"
  ],
  "author": "Saran Tanpituckpong <sarunta@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gluons/ConfC/issues"
  },
  "homepage": "https://github.com/gluons/ConfC",
  "dependencies": {
    "chalk": "^2.3.0",
    "fs-extra": "^4.0.2",
    "inquirer": "^4.0.1",
    "js-yaml": "^3.6.1",
    "p-series": "^1.0.0",
    "rc": "^1.2.2",
    "yargs": "^10.0.3"
  },
  "devDependencies": {
    "@types/fs-extra": "^4.0.5",
    "@types/js-yaml": "^3.10.1",
    "@types/node": "8",
    "chai": "*",
    "mocha": "*",
    "rimraf": "^2.6.2",
    "tslint": "^5.8.0",
    "tslint-config-gluons": "^1.0.5",
    "typescript": "^2.6.2"
  }
}
